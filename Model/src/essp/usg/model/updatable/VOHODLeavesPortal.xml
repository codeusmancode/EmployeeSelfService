  <SQLQuery><![CDATA[SELECT EOHODLeavesPortal.APPROVED,                  
       EOHODLeavesPortal.CANCEL,                  
       EOHODLeavesPortal.CREATED_BY,                  
       EOHODLeavesPortal.CREATION_DATE,                  
       EOHODLeavesPortal.DEPT_ID,                  
       EOHODLeavesPortal.EMP_CODE,                  
       EOHODLeavesPortal.EMP_ID,                  
       EOHODLeavesPortal.FROM_DATE,      
       EOHODLeavesPortal.leavetype lv_type_code,   
       DECODE(EOHODLeavesPortal.leavetype,'CL','Casual Leave (CL)','SL','Sick Leave (SL)','CLH','Half Day (CL)','SLH','Half Day (SL)','PL','Privleged Leave (PL)','PLH','Half Day (PL)','SHL','Short Leave','OD','Official Work','CPL','Compensatory Leave (CPL)','CPLH','Half Day (CPL)') LEAVETYPE,                  
       EOHODLeavesPortal.REMARKS,                  
       EOHODLeavesPortal.SR_NO,                  
       EOHODLeavesPortal.TO_DATE,  
       to_char(EOHODLeavesPortal.FROM_DATE,'HH:MI: AM') from_t, 
                      to_char(EOHODLeavesPortal.TO_DATE,'HH:MI: AM') to_t, 
       EOHODLeavesPortal.TOT_LVS,                  
       EOHODLeavesPortal.UPDATED_DATE,               
       EOHODLeavesPortal.VERSION_NUMBER,              
       emp.hod,                 
       emp.name   ,        
        emp.mail_id        
FROM  ustms.LEAVES_PORTAL EOHODLeavesPortal                 
      ,USPR.employees_all emp                 
where emp.emp_id = EOHODLeavesPortal.emp_id                 
and emp.hod = :emp_id             
and NVL(EOHODLeavesPortal.approved,'N')='N'     
and  nvl(EOHODLeavesPortal.CANCEL,'N') <> 'Y'    
and trunc(EOHODLeavesPortal.creation_date) > to_date('1-jan-2018')            
and not exists (select * from leaves_portal_hr hr where hr.emp_id =EOHODLeavesPortal.emp_id and hr.from_date = EOHODLeavesPortal.from_Date and hr.approved = 'Y' )          
order by EOHODLeavesPortal.creation_date desc]]></SQLQuery>
